    "2020/walensee": {
        "title": "Walensee",
        "region": "St. Gallen",
        "country": "Switzerland",
        "year": "2020",
        "month": "May",
        "category": "Lake Tour",
        "distance": 7.5,
        "launch": "Murg",
        "time_h": 1,
        "time_m": 53,
        "track": "track.kml",
        "map_latitude": 47.2098,
        "map_longitude": 8.82551,
        "map_zoom": 13,
        "text": "The Walensee is one of the most beautiful lakes in Switzerland. Having a Fjord-like shape, it is 15 kilometers long and less than 2 kilometers wide. On its northern shore, the lake is bounded by steep mountain walls that tower more than 1600 meters above the surface. The lake offers great opportunities for canoeing, but due to suddenly occuring strong winds and a lack of possibilities to disembark at its northern shore the beauty of the lake can also be a treacherous. We did a short afternoon trip on the lake, launching the boat near the beach in Murg. For the first hour we fought against strong headwinds and high waves but the lake turned calm later in the afternoon.",
        "coverimage": "NA",
        "images": {
            "DSC_7763.jpg": "Lonely Bay",
            "DSC_7759.jpg": "Churfirsten",
            "DSC_7713.jpg": "Walensee East View",
            "DSC_8270.jpg": "Canoe",
            "DSC_8233.jpg": "Murg Beach",
            "DSC_7950.jpg": "Quinten"
        }
    },
